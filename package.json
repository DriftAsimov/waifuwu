{
	"name": "waifuwu",
	"displayName": "waifuwu",
	"description": "Enjoy waifus' company while you code!",
	"version": "0.1.0",
	"publisher" : "DriftAsimov",
	"repository": {
		"type" : "git",
		"url" : "https://github.com/DriftAsimov/waifuwu"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color" : "#76D3E0",
		"theme" : "dark"
	},
	"homepage": "https://github.com/DriftAsimov/waifuwu/blob/main/README.md",
	"bugs": {
		"url": "https://github.com/DriftAsimov/waifuwu/issues",
		"email": "driftasimov@gmail.com"
	},
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"keywords": ["anime", "waifu", "waifuwu", "otaku", "cute"],
	"license": "SEE LICENSE IN LICENSE",
	"activationEvents": [
		"onCommand:waifuwu.getWaifu"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "waifuwu.getWaifu",
				"title": "Get Waifu"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"node-fetch": "^2.6.1"
	}
}
